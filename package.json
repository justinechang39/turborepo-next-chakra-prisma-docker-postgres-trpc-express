{
  "name": "turborepo-try",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/**",
    "packages/**"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "bash clean_docker.sh && npm run db:init -w database && turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "npm@8.15.0"
}
